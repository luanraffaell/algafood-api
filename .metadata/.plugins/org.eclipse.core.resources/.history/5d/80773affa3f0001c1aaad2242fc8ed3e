package com.algafood.domain.repository;

import java.util.List;

public interface FormaDePagamento {
	List<FormaDePagamentoRepository> listar();
	FormaDePagamentoRepository buscar(Long id);
	FormaDePagamentoRepository salvar(FormaDePagamentoRepository forma);
	void remover(FormaDePagamentoRepository forma);
	
}
