package com.algafood.domain.repository;

import java.util.List;

import com.algafood.domain.model.FormaDePagamento;

public interface FormaDePagamentoRepository {
	List<FormaDePagamento> listar();
	FormaDePagamento buscar(Long id);
	FormaDePagamento salvar(FormaDePagamento forma);
	void remover(FormaDePagamento forma);
	
}
